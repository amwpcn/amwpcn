{
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "start": {
      "dependsOn": [
        "^start"
      ]
    },
    "test": {
      "cache": true,
      "outputs": [
        "{projectRoot}/../../coverage"
      ]
    }
  }
}
